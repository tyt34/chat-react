{
  "name": "reactjs",
  "version": "0.1.0",
  "private": true,
  "homepage": "https://tyt34.github.io/chat-react",
  "dependencies": {
    "@reduxjs/toolkit": "^1.8.6",
    "node-sass": "^8.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-dotenv": "^0.1.3",
    "react-redux": "^8.0.4",
    "react-router-dom": "^6.4.2",
    "react-scripts": "5.0.1",
    "redux": "^4.2.0",
    "socket.io-client": "^4.5.3",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "build": "DISABLE_ESLINT_PLUGIN=true react-scripts build",
    "build-w": "react-scripts build",
    "deploy": "echo 'REACT_APP_URLAPI=prod' > .env; gh-pages -d build",
    "eslint": "eslint 'src/**/*.tsx' --fix",
    "pret": "npx prettier --write .",
    "start": "echo 'REACT_APP_URLAPI=test' > .env; react-dotenv && DISABLE_ESLINT_PLUGIN=true react-scripts start",
    "start-w": "react-scripts start",
    "test": "jest --config jest.config.json",
    "c": "jest --coverage --config jest.config.json"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@testing-library/dom": "^9.3.1",
    "@testing-library/jest-dom": "^6.1.3",
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/jest": "^29.5.4",
    "@types/node": "^16.18.0",
    "@types/react": "^18.0.23",
    "@types/react-dom": "^18.0.7",
    "@types/react-redux": "^7.1.24",
    "@types/react-router-dom": "^5.3.3",
    "@types/socket.io": "^3.0.2",
    "@typescript-eslint/eslint-plugin": "^5.41.0",
    "@typescript-eslint/parser": "^5.41.0",
    "babel-jest": "^29.6.4",
    "eslint": "^8.26.0",
    "eslint-plugin-react": "^7.31.10",
    "gh-pages": "^4.0.0",
    "jest": "^29.6.4",
    "jest-environment-jsdom": "^29.6.4",
    "jest-preview": "^0.3.1",
    "prettier": "2.8.7",
    "sass": "^1.67.0",
    "ts-jest": "^29.1.2",
    "typescript": "^4.9.5"
  }
}
